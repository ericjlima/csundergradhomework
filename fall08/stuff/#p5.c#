#include <stdio.h>

int main(int argc,char *arcv[]){

  int a,b,c;

  printf("Please, enter the two numbers you wish to add.\n");

  scanf("%d%d",&a,&b);

  c=a+b;

  printf("The sum of these two numbers is %d.\n",c);

  return 0;
}
